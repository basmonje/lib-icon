{
  "name": "@basmonje/lib-icon",
  "version": "1.0.3",
  "main": "dist/index.d.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/basmonje/lib-icon"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf src/components && rimraf dist",
    "build": "npm run svgr && rollup -c",
