    "@rollup/plugin-node-resolve": "^13.0.6",
    "@svgr/cli": "^5.5.0",
    "@types/react": "^17.0.33",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.3",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
